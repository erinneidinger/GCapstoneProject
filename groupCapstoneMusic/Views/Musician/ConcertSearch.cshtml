@model IEnumerable<groupCapstoneMusic.Models.Concert>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ConcertSearch</title>
</head>

<h2>My Gigs</h2>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Venue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConcertDate)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Venue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConcertDate)
        </td>
        <td>
            @Html.ActionLink("Details", "ConcertDetailsForMusician", new { id = item.Id })
        </td>
        <td>
            @Html.ActionLink("Hosts Profile", "CustomerProfileForMusician", new { id = item.ApplicationId })
        </td>
    </tr>
    }
    
    </table>
</body>
</html>
