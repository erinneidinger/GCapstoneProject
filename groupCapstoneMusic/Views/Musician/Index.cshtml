        <th><p><a class="btn btn-success" @Html.ActionLink("Details", "Details", new { id = Model.ID })</a></p></th>
        <th><p><a class="btn btn-danger" @Html.ActionLink("Delete", "Delete", new { id = Model.ID })</a></p></th>
