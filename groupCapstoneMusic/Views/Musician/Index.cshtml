            <td>
                @Html.DisplayFor(modelItem => item.Rating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SetRate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DatesAvailable)
            </td>
