@model groupCapstoneMusic.Models.Musician

@{
    ViewBag.Title = "Index";
}


<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<link rel="stylesheet" href="~/Content/Site.css" />




<div class="container">
    <div class="row">
        <div class="jumbotron">

            <div class="card hovercard">
                <div class="cardheader">
                    <img src="@Model.bannerPic" alt="@Model.bannerPic" style="background-size: auto; height: 135px;" width="450" ;>

                </div>
                <div class="avatar">
                    <img src="@Model.ImageURL" alt="@Model.ImageURL">
                </div>
                <div class="info">
                    <div class="title">
                        <a>@Html.DisplayFor(model => model.FirstName)</a>
                    </div>
                    <div>@Html.DisplayNameFor(model => model.StreetAddress)||@Html.DisplayFor(model => model.StreetAddress)</div>
                    <div>@Html.DisplayNameFor(model => model.City)||@Html.DisplayFor(model => model.City)</div>
                    <div>@Html.DisplayNameFor(model => model.State)||@Html.DisplayFor(model => model.State)</div>
                    <div>@Html.DisplayNameFor(model => model.Email)||@Html.DisplayFor(model => model.Email)</div>
                    <div><h3>@Html.DisplayFor(model => model.Bio)</h3></div>
                </div>
                <div class="bottom">
                    <a class="btn btn-primary btn-twitter btn-sm" href="https://twitter.com/webmaniac" style="color:lightblue">
                        <i class="fa fa-twitter"></i>
                    </a>
                    <a class="btn btn-primary btn-sm" rel="publisher"
                       href="https://plus.google.com/shahnuralam" style="color:blue">
                        <i class="fa fa-facebook"></i>
                    </a>
                    <a class="btn btn-primary btn-sm" rel="publisher"
                       href="https://plus.google.com/shahnuralam" style="color:deeppink">
                        <i class="fa fa-instagram"></i>
                    </a>
                </div>
            </div>
        </div>
        <table>
            <tr>
                <th><p><a class="btn btn-primary" style="color:lightgreen" @Html.ActionLink("Reviews", "Details", "Review", new { id = Model.ApplicationId }, null)</a></p></th>
                <th><p><a class="btn btn-primary" style="color:yellow" @Html.ActionLink("Edit", "Edit", new { id = Model.ID })</a></p></th>
                <th><p><a class="btn btn-primary" style="color:yellow" @Html.ActionLink("Details", "Details", new { id = Model.ID })</a></p></th>
                <th><p><a class="btn btn-primary" style="color:red" @Html.ActionLink("Delete", "Delete", new { id = Model.ID })</a></p></th>
            </tr>
            </table>
    </div>
</div>




<p align="center">
    <iframe width="420" height="345" src="@ViewBag.URL">
    </iframe>
</p> @*YoutubeVideo*@

