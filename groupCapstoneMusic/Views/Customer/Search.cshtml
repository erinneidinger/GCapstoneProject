@model groupCapstoneMusic.Models.CustomerMusiciansViewModel

@{
    ViewBag.Title = "SearchParameters";
}

<h1>Search</h1>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.musicians[0].FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.musicians[0].LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.musicians[0].BandName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.musicians[0].Genre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.musicians[0].City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.musicians[0].SetRate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.musicians)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                <script>
                if (@Html.DisplayFor(modelItem => item.BandName != null))
                {
                    @Html.DisplayFor(modelItem => item.BandName)
                }

                </script>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SetRate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>
</body>
</html>
